<div class="row">
</div>
<div class="row">
  <div class="col s12">
    <div class="card-panel z-depth-3">
      <h2>Here's <%= @user.username %>'s 5 most recent articles'!</h2>
      <% @articles.each do |cat| %>
      <a href="/articles/<%= cat.id %>">
        <div class="collection">
          <%= cat.title %>
        </div>
      </a>
      <% end %>
    </div>
  </div>
</div>
